{
  "name": "lclite-poc",
  "license": "MIT",
  "description": "Rediscovery Test",
  "keywords": [
    "react",
    "redux",
    "node",
    "sass"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:noeliasfranco/rediscovery.git"
